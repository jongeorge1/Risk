<input id="id" type="hidden" value="<%= this.id %>" />
<li data-icon="false">
	<a>
	<div class="overflow">
  <strong>Name: </strong><%=this.Name%>
  </div>
  </a>
</li>
<li data-icon="false">
	<a>
	<div class="overflow">
  <strong>Contact Name: </strong><%=this.ContactName%>
  </div>
  </a>
</li>
<li>
<a href="tel:<%=this.Telephone%>">
<div class="overflow">
  <strong>Telephone: </strong><%=this.Telephone%>
  </div>
  </a>
</li>
<li data-theme="d">
	<% if(this.Email){ %>
	<a href="mailto:<%=this.Email%>">
	<div class="overflow">
	<strong>Email: </strong><%=this.Email%>
	</div>
	</a> 
	<% } else { %>
	<a href="#">
	<strong>Email: </strong>
	</a>
	<% } %>
</li>
<li>
	<a target="_blank" href="http://maps.google.com/maps?q=<%=this.Postcode%>, UK">
		<div class="overflow">
			<strong>Address: </strong>
			<%=this.Street%> <%=this.Town%> <%=this.County%> <%=this.Postcode%>	
		</div>
	</a>
</li>

